<% array = ["your name", "your email", "tests"]%>

<%= form_with model: @user do |f| %>
<%= f.label :username, "username" %>
<%= f.text_field :username %>
<%= f.label :email, "email" %>
<%= f.email_field :email, placeholder: array.sample, value: array.sample %>
<%= f.label :password, "Password"%>
<%= f.password_field :password %>
<%= f.submit "Submit" %>
<%end%>